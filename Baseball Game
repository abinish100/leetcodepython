class Solution(object):
    def calPoints(self, operations):
        a=[]
        for i in operations:
            if i=="C":
                a.remove(a[-1])
            elif i=="D":
                a.append(a[-1]*2)
            elif i=="+":
                a.append(a[-2]+a[-1])
            else:
                a.append(int(i)) 
        return sum(a)
