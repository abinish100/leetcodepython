class Solution(object):
    def duplicateZeros(self, arr):
        a1=[]
        j=0
        for i in range(len(arr)):
            if arr[i]==0:
                a1.append(0)
                a1.append(0)
            else:
                a1.append(arr[i])
        for i in range(len(arr)):
            arr[i]=a1[i]
