class Solution(object):
    def findErrorNums(self, nums):
        a1=[]
        a2=[]
        s=sorted(nums)
        a3=[]
        for i in range(1,len(nums)+1):
            a1.append(i)
            if i not in nums:
                a3.append(i)
                
        counter = Counter(nums)

        for num, freq in counter.items():
            if freq == 2:
                a2.append(num)
            
        

            
                
        return a2+a3


        
